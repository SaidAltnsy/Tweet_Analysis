#PIG LATIN
def pig_latin(word):
    first_letter=word[0]
    #check if vowel
    if first_letter in 'aeiou':
        pig_word=word+'ay'
    else:
        pig_word=word[1:]+word[0]+'ay'
        
    return pig_word    
        


